{
  "name": "acme",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run prettier && set NG_PERSISTENT_BUILD_CACHE=1",
    "start": "ng serve --port=4200 --open=true --ssl --ssl-key=extras/webserver/private-key.pem --ssl-cert=extras/webserver/certificate.cer",
    "start:http": "ng serve --port=4200",
    "start:prod": "ng serve --port=4200 --configuration production",
    "start:de": "ng serve --configuration=de --port=4200 --open=true --ssl --ssl-key=extras/webserver/private-key.pem --ssl-cert=extras/webserver/certificate.cer",
    "start:ORIG": "ng serve",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && cypress run --browser chrome",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "build:watch": "ng build --configuration=development --watch",
    "browserslist": "browserslist",
    "browserslist:update": "npx browserslist@latest --update-db",
    "clean": "rimraf dist doc",
    "cypress": "cypress open",
    "eslint": "eslint --ext .ts src",
    "eslint:cypress": "eslint --ext .js cypress/integration",
    "i18n": "ng xi18n --output-path=src/locale --out-file=messages.de.xlf",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "lighthouse:hs": "lighthouse https://www.h-ka.de/home/ --view --output-path=temp/www.h-ka.de.html",
    "lighthouse:hskampus": "lighthouse http://www.iwi.h-ka.de/hskampus-web/ --view --output-path=temp/app.hskampus.de.html --chrome-flags='--disable-web-security'",
    "local-web-server": "ws",
    "prepack": "npm run build:dev",
    "pack": "pack build juergenzimmermann/buchweb:1.0.0 --builder paketobuildpacks/builder:base",
    "prettier": "prettier --write src/**/*.ts src/**/*.html src/**/*.scss",
    "presource-map-explorer": "npm run build -- --source-map && del dist/acme/ngsw-worker.js && del dist/acme/safety-worker.js && del dist/acme/worker-basic.min.js",
    "source-map-explorer": "source-map-explorer dist/acme/main.*.js dist/acme/main.*.js.map --html source-map-explorer.html --only-mapped",
    "stylelint": "stylelint src/app/**/*.scss",
    "typedoc": "typedoc --verbose --options typedoc.json src",
    "webhint": "hint src",
    "prewebpack-bundle-analyzer": "npm run build -- --stats-json",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/acme/stats.json dist/acme --port 9999"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.0",
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/localize": "14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/pwa": "14.0.0",
    "@angular/router": "14.0.0",
    "@angular/service-worker": "14.0.0",
    "@js-temporal/polyfill": "0.4.1",
    "@popperjs/core": "2.11.5",
    "@swimlane/ngx-charts": "20.1.0",
    "angular-calendar": "0.29.0",
    "bootstrap": "5.1.3",
    "date-fns": "2.28.0",
    "loglevel": "1.8.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.7",
    "@angular/compiler-cli": "14.0.0",
    "@angular/language-service": "14.0.0",
    "@types/d3-scale": "4.0.2",
    "@types/d3-selection": "3.0.2",
    "@types/node": "17.0.27",
    "@types/web": "0.0.62",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "cypress": "9.6.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-rxjs": "5.0.2",
    "eslint-plugin-unicorn": "42.0.0",
    "hint": "6.1.11",
    "lighthouse": "9.5.0",
    "local-web-server": "5.2.0",
    "lws-redirect": "2.1.2",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "source-map-explorer": "2.5.2",
    "stylelint": "14.7.1",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "25.0.0",
    "typescript": "4.6.3",
    "webpack-bundle-analyzer": "4.5.0"
  }
}
